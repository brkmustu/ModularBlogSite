{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MongoDBBson",
        "Args": {
          "databaseUrl": "mongodb://localhost:27017/logs",
          "collectionName": "UserPortalModuleLogs",
          "cappedMaxSizeMb": "500",
          "cappedMaxDocuments": "1000"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "UserPortalModule": "Server=127.0.0.1;Port=5432;Database=UserPortalModuleDb;User Id=postgres;Password=deneme;CommandTimeout=30;"
  },
  "SystemOptions": {
    "SeedSampleData": true
  },
  "AllowedHosts": "*"
}

